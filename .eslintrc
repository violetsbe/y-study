{
  "parserOptions": {
    "esmaVersion": 2020, // es6 버전에 따르겠다
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true // react
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["import", "react", "react-hooks"]
}
